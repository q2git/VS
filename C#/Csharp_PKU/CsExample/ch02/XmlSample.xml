<?xml version="1.0"?>
<doc>
    <assembly>
        <name>XmlSample</name>
    </assembly>
    <members>
        <member name="T:SomeClass">
            <summary>
            这里可以写有关类的注释.</summary>
            <remarks>
            一些更长更详细的注释， 
            可以通过remarks标记来表示</remarks>
        </member>
        <member name="F:SomeClass.myName">
            <summary>
            名字属性</summary>
        </member>
        <member name="M:SomeClass.#ctor">
            <summary>
            构造方法 </summary>
        </member>
        <member name="M:SomeClass.SomeMethod(System.String)">
            <summary>
            对 SomeMethod 方法的注释.</summary>
            <param name="s"> 参数 s 的描述在这里</param>
            <seealso cref="T:System.String">
            用 cref 可以表明参见其他内容 
            编译器会自动检查相关的内容是否存在. </seealso>
        </member>
        <member name="M:SomeClass.SomeOtherMethod">
            <summary>
            其他方法. </summary>
            <returns>
            用 returns 标记可以表明返回值.</returns>
            <seealso cref="M:SomeClass.SomeMethod(System.String)">
            这里用 cref 表示参考相关的一个方法 </seealso>
        </member>
        <member name="M:SomeClass.Main(System.String[])">
            <summary>
            应用程序的入口
            </summary>
            <param name="args"> 命令行参数 </param>
        </member>
        <member name="P:SomeClass.Name">
            <summary>
            名字属性 Name </summary>
            <value>
            value 标记用于描述属性值</value>
        </member>
    </members>
</doc>
